{
      "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "strict": true,
        "jsx": "preserve",
        "esModuleInterop": true,
        "allowJs": true,
        "skipLibCheck": true,
        "types": ["@nuxt/types", "@nuxtjs/axios", "@types/node"]
      },
      "exclude": ["node_modules", ".nuxt", "dist"]
    }